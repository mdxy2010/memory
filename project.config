	submit = group user/mdxy2010
[access "refs/heads/*"]
	read = group user/mdxy2010
	create = group user/mdxy2010
	push = +force group user/mdxy2010
	pushMerge = group user/mdxy2010
[access "refs/tags/*"]
	push = group user/mdxy2010
	push = +force group user/mdxy2010
